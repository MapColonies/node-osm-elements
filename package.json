{
  "name": "node-osm-elements",
  "version": "1.0.0",
  "description": "TypeScript and JavaScript classes for osm elements",
  "main": "index.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "release": "standard-version",
    "build": "tsc --project tsbuildconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MapColonies/node-osm-elements.git"
  },
  "author": "galta95",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MapColonies/node-osm-elements/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "eslintConfig": {
    "extends": [
      "@map-colonies/eslint-config/ts-base"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/MapColonies/node-osm-elements#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@map-colonies/eslint-config": "^1.1.0",
    "@types/node": "^14.14.12",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "commitlint": "^11.0.0",
    "eslint": "^7.15.0",
    "husky": "^4.3.5",
    "prettier": "^2.2.1",
    "standard-version": "^9.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  }
}
